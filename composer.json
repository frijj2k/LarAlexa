{
    "name": "frijj2k/laralexa",
    "description": "Set of classes to make creating simple Amazon Echo Alexa Apps easier with Lumen and to a lesser extent Laravel",
    "license": "MIT",
    "keywords": ["echo", "echo dot", "Amazon", "Amazon Echo", "Alexa", "LarAlexa", "frijj2k", "PDS", "Lumen", "Laravel"],
    "authors": [
        {
            "name": "Dave Pearson",
            "email": "dave@pds-uk.com"
        }
    ],
    "require": {
         "php": ">=5.5.9",
         "illuminate/routing": "5.2.*|5.3.*"
     },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Frijj2k\\LarAlexa\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
